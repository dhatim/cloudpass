{
  "name": "cloudpass",
  "version": "1.2.0",
  "private": true,
  "description": "User management system",
  "keywords": [
    "user management"
  ],
  "author": {
    "email": "msuret@dhatim.com",
    "name": "Maxime Suret"
  },
  "homepage": "https://github.com/dhatim/cloudpass/issues",
  "bugs": {
    "url": "https://github.com/dhatim/cloudpass/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhatim/cloudpass.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.30.2",
    "npm": ">=2.13.1"
  },
  "main": "src/main.js",
  "dependencies": {
    "bcryptjs": "2.3.0",
    "body-parser": "1.15.0",
    "compression": "1.6.1",
    "config": "1.19.0",
    "continuation-local-storage": "3.1.6",
    "cookie-parser": "1.4.1",
    "express": "4.13.4",
    "js-yaml": "3.5.4",
    "jsonwebtoken": "5.7.0",
    "lodash": "4.6.1",
    "moment": "2.12.0",
    "morgan": "1.7.0",
    "nodemailer": "2.3.0",
    "optional-js": "1.1.1",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "pg": "4.5.1",
    "pg-hstore": "2.3.2",
    "randomstring": "1.1.4",
    "sequelize": "3.19.3",
    "sqlite3": "3.1.1",
    "ssacl": "0.1.3",
    "swagger-express-mw": "0.6.0",
    "throw": "1.1.0",
    "umzug": "1.9.1"
  },
  "devDependencies": {
    "bootprint": "0.8.3",
    "bootprint-openapi": "0.14.0",
    "coveralls": "2.11.8",
    "env-test": "1.0.0",
    "gh-pages-with-token": "1.0.0",
    "gulp": "3.9.1",
    "gulp-deb": "0.2.0",
    "gulp-rename": "1.2.2",
    "istanbul": "0.4.2",
    "merge-stream": "1.0.0",
    "mocha": "2.4.5",
    "rewire": "2.5.1",
    "rimraf": "2.5.2",
    "smtp-tester": "0.5.1",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.0.0"
  },
  "scripts": {
    "start": "node .",
    "clean": "rimraf build",
    "test:unit": "istanbul cover --config=tests/unit/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts tests/unit",
    "test:integration": "istanbul cover --config=tests/integration/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts tests/integration",
    "test": "npm run test:unit && npm run test:integration",
    "coveralls": "cat build/reports/coverage/integration/lcov.info | coveralls",
    "api-doc": "bootprint openapi swagger/swagger.yaml build/reports/api-doc",
    "gh-pages": "gh-pages-with-token -d build/reports/api-doc",
    "deb": "gulp deb"
  }
}
