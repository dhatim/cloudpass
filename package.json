{
  "name": "cloudpass",
  "version": "1.2.2",
  "private": true,
  "description": "User management system",
  "keywords": [
    "user management"
  ],
  "author": {
    "email": "msuret@dhatim.com",
    "name": "Maxime Suret"
  },
  "homepage": "https://github.com/dhatim/cloudpass/issues",
  "bugs": {
    "url": "https://github.com/dhatim/cloudpass/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhatim/cloudpass.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.3.2",
    "npm": ">=2.13.1"
  },
  "main": "src/main.js",
  "dependencies": {
    "bcryptjs": "2.3.0",
    "body-parser": "1.15.1",
    "compression": "1.6.1",
    "config": "1.20.1",
    "continuation-local-storage": "3.1.7",
    "cookie-parser": "1.4.1",
    "express": "4.13.4",
    "js-yaml": "3.6.1",
    "jsonwebtoken": "6.2.0",
    "lodash": "4.12.0",
    "moment": "2.13.0",
    "morgan": "1.7.0",
    "nodemailer": "2.4.0",
    "optional-js": "1.1.2",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-http-bearer": "1.0.1",
    "pg": "4.5.5",
    "pg-hstore": "2.3.2",
    "randomstring": "1.1.4",
    "sequelize": "3.23.1",
    "ssacl": "0.1.3",
    "swagger-express-mw": "0.6.0",
    "throw": "1.1.0",
    "umzug": "1.11.0"
  },
  "devDependencies": {
    "bootprint": "0.8.5",
    "bootprint-openapi": "0.16.0",
    "coveralls": "2.11.9",
    "dockerode": "2.2.10",
    "dockerode-promise": "0.1.0",
    "env-test": "1.0.0",
    "gh-pages-travis": "1.0.0",
    "gulp": "3.9.1",
    "gulp-deb": "0.2.1",
    "gulp-rename": "1.2.2",
    "istanbul": "0.4.3",
    "jshint": "2.9.2",
    "merge-stream": "1.0.0",
    "mocha": "2.4.5",
    "rewire": "2.5.1",
    "rimraf": "2.5.2",
    "smtp-tester": "0.6.1",
    "sqlite3": "3.1.3",
    "superagent": "1.8.3",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "tar-fs": "1.12.0",
    "thenify": "3.2.0"
  },
  "scripts": {
    "start": "node .",
    "clean": "rimraf build",
    "test:unit": "istanbul cover --config=tests/unit/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts tests/unit",
    "test:integration": "istanbul cover --config=tests/integration/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts tests/integration",
    "lint": "jshint src",
    "pretest": "npm run lint",
    "test": "istanbul cover --config=tests/.istanbul.yml node_modules/mocha/bin/_mocha -- --opts tests/mocha.opts tests",
    "coveralls": "cat build/reports/coverage/global/lcov.info | coveralls",
    "api-doc": "bootprint openapi swagger/swagger.yaml build/reports/api-doc",
    "gh-pages": "gh-pages-travis",
    "deb": "gulp deb",
    "deploy-deb": "gulp deploy-deb",
    "build-docker-image": "gulp build-docker-image",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "nocomma": true,
    "nonew": true,
    "strict": true,
    "undef": true,
    "unused": true,
    "validthis": true
  }
}
